        <div class="description">
          <h1>Tel Aviv-Yafo</h1>
          <h3>84 kilometeres away</h3>
        </div>
      </div>
      <div class="mainPage__citySuggestions__cards__card" >
        <img src="https://mir-s3-cdn-cf.behance.net/project_modules/disp/014a9043643817.623277932bd44.gif" data-card="Eilat">
        <div class="description">
          <h1>Eilat</h1>
          <h3>84 kilometeres away</h3>
        </div>
      </div>
      <div class="mainPage__citySuggestions__cards__card" >
        <img
          src="https://4.bp.blogspot.com/-pMoMk50GlQI/WxuejAtymnI/AAAAAAAACAY/4YGpCuqLuY4BSn2jkcImneBW3X-GCx6ewCLcBGAs/s400/happy-town-gif1b.gif" data-card="Jerusalem">
        <div class="description">
          <h1>Jerusalem</h1>
          <h3>84 kilometeres away</h3>
        </div>
      </div>
      <div class="mainPage__citySuggestions__cards__card" >
        <img src="https://i.pinimg.com/originals/11/c0/0c/11c00c702d268a5a7789e801f995fbcc.gif" data-card="Harei Yehuda">
        <div class="description">
          <h1>Harei Yehuda</h1>
          <h3>84 kilometeres away</h3>
        </div>
      </div>

    </div>
  </div>

<div class="bottomBar">
  <%- include('./components/bottomBar') %>
</div>
<p class="airbnbRights" >© 2022 Airbnb, Inc.Privacy·Terms·Sitemap</p> 

  <script src="dist/index.js"></script>
